{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "ajv": "^6.5.5",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.9.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "rc-tabs": "^9.4.8",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-jsonschema-form": "^1.0.6",
    "react-intl": "^2.7.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "react-tabulator": "^0.5.10",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "sass": "^1.9.0",
    "stylelint": "^8.2.0",
    "stylelint-config-rational-order": "0.0.2",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "tabulator-tables": "^4.1.1"
  },
  "devDependencies": {
    "@types/react": "^16.4.18",
    "standard": "^10.0.3",
    "prop-types": "latest"
  },
  "scripts": {
    "start": "npm-run-all -p watch-styles-sass start-js",
    "build": "npm run stylelint-build && react-scripts build",
    "start-js": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.js\"",
    "stylelint-dev": "stylelint src/**/*.scss || true --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint-build": "stylelint 'src/**/*.scss'",
    "watch-styles-sass": "npm-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ]
}
