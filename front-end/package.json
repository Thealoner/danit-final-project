{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/*.scss"
      ],
      "extensions": "scss"
    }
  },
  "dependencies": {
    "node-sass": "^4.9.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "sass": "^1.9.0",
    "stylelint": "^8.2.0",
    "stylelint-config-rational-order": "0.0.2",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0"
  },
  "devDependencies": {
    "standard": "^10.0.3"
  },
  "scripts": {
    "start": "npm-run-all -p watch-styles-sass start-js",
    "build": "npm run stylelint-build && react-scripts build",
    "start-js": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stylelint-dev": "stylelint src/*.scss || true --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint-build": "stylelint 'src/*.scss'",
    "watch-styles-sass": "npm-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ]
}
